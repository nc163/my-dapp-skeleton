{
  "files.watcherExclude": {
    "**/.vscode": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "out": true
  },
  //
  "search.exclude": {
    "**/.vscode": true,
    "**/node_modules": true
  },
  //
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "[markdown]": {},
  "[json]": {}
}
